{
  "name": "voice-chat-assistant-backend",
  "version": "0.1.0",
  "description": "Backend for voice coding assistant",
  "main": "dist/src/main.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/src/main.js",
    "dev": "tsx watch src/main.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:strict": "tsc && tsc-alias",
    "build:agentos": "tsc && tsc-alias",
    "postbuild": "node ./scripts/postbuild/copy-js.cjs && node ./scripts/postbuild/flatten-dist.cjs && node ./scripts/postbuild/ensure-esm-extensions.cjs",
    "pretest": "pnpm --filter @framers/agentos-ext-http-api build",
    "test": "node --test --import tsx --experimental-specifier-resolution=node src/__tests__/agency.integration.test.ts src/__tests__/agentos.org-memory.test.ts src/__tests__/agentos.memory-redactions.test.ts src/__tests__/agentos.multimodal-rag.test.ts src/__tests__/agentos.graphrag.update.test.ts src/__tests__/agentos.rag.keyword-bm25.test.ts src/__tests__/agentos.rag.multiquery.test.ts src/__tests__/agentos.rag.http.e2e.test.ts src/__tests__/appDatabase.test.ts src/__tests__/common.infrastructure.test.ts src/__tests__/otel-logs.test.ts src/__tests__/extensions.service.test.ts src/__tests__/guardrails.service.test.ts"
  },
  "dependencies": {
    "@framers/agentos": "workspace:*",
    "@framers/agentos-ext-http-api": "workspace:*",
    "@framers/agentos-extensions-registry": "workspace:*",
    "@framers/agentos-skills-registry": "workspace:*",
    "@framers/shared": "workspace:*",
    "@framers/sql-storage-adapter": "workspace:*",
    "@nestjs/common": "^11.1.13",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.13",
    "@nestjs/platform-express": "^11.1.13",
    "@nestjs/platform-socket.io": "^11.1.13",
    "@nestjs/swagger": "^11.2.6",
    "@nestjs/websockets": "^11.1.13",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.201.1",
    "@opentelemetry/auto-instrumentations-node": "^0.59.0",
    "@opentelemetry/sdk-node": "^0.201.1",
    "@supabase/supabase-js": "^2.45.1",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "graphology": "^0.26.0",
    "graphology-communities-louvain": "^2.0.2",
    "graphology-types": "^0.24.8",
    "helmet": "^7.1.0",
    "i18next": "^25.2.1",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.4",
    "ioredis": "^5.6.1",
    "jsonrepair": "^3.5.2",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.2.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "pino": "^9.14.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "tslib": "^2.8.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.13",
    "@opentelemetry/context-async-hooks": "^2.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^0.7.33",
    "@types/multer": "^1.4.10",
    "@types/node": "^20.10.0",
    "cross-env": "^7.0.3",
    "es-module-lexer": "^1.7.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/framersai/agentos-backend"
  }
}
