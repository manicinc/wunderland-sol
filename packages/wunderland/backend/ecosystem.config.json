[
    {
      "name": "voice-backend",
      "script": "dist/server.js",
      "exec_mode": "fork",
      "instances": 1,
      "watch": false,
      "ignore_watch": ["node_modules", "logs"],
      "node_args": ["--experimental-specifier-resolution=node"],
      "output": "/root/.pm2/logs/voice-backend-output.log",
      "error": "/root/.pm2/logs/voice-backend-error.log",
      "env": {
        "NODE_ENV": "production",
        "PORT": 3333
      },
      "env_development": {
        "NODE_ENV": "development",
        "PORT": 3333
      }
    }
  ]
