{
  "name": "@framersai/frame.dev",
  "version": "0.2.0",
  "private": true,
  "description": "Frame - Denoising the web. The OS for your life.",
  "main": "electron-dist/main.js",
  "scripts": {
    "dev": "next dev",
    "dev:api": "tsx watch server.ts",
    "dev:all": "concurrently \"pnpm dev\" \"pnpm dev:api\"",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=6144 node scripts/copy-onnx-wasm.js && node scripts/copy-sqljs-wasm.js && node scripts/copy-esbuild-wasm.js && node scripts/copy-pdfjs-worker.js && node scripts/download-semantic-model.js && node scripts/generate-suggested-questions.js && node scripts/static-build.js",
    "build:next": "cross-env STATIC_EXPORT=true next build",
    "start": "next start",
    "start:api": "tsx server.ts",
    "api": "tsx server.ts",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:integration": "vitest run __tests__/integration",
    "test:components": "vitest run --config vitest.config.components.mts",
    "test:api": "vitest run __tests__/api",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:coverage": "vitest run --coverage",
    "test:badges": "node scripts/update-test-badges.js",
    "test:all": "pnpm test:unit && pnpm test:components",
    "postinstall": "node scripts/copy-onnx-wasm.js && node scripts/copy-sqljs-wasm.js && node scripts/copy-esbuild-wasm.js && node scripts/copy-pdfjs-worker.js && node scripts/download-semantic-model.js",
    "questions": "node scripts/generate-suggested-questions.js",
    "embeddings": "node scripts/generate-embeddings.js",
    "embeddings:full": "node scripts/generate-embeddings.js && node scripts/generate-suggested-questions.js",
    "vocabulary:embeddings": "npx tsx scripts/generate-vocabulary-embeddings.ts",
    "prebuild:vocabulary": "npx tsx scripts/generate-vocabulary-embeddings.ts",
    "rebuild:index": "npx tsx scripts/rebuild-full-index.ts",
    "electron:dev": "concurrently \"pnpm dev\" \"wait-on http://localhost:3000 && pnpm electron:start\"",
    "electron:start": "cross-env NODE_ENV=development ELECTRON_RUN_AS_NODE= electron .",
    "electron:build": "tsc -p electron/tsconfig.json",
    "electron:prebuild": "cross-env ELECTRON_BUILD=true NEXT_PUBLIC_DEPLOYMENT_MODE=offline NEXT_PUBLIC_EDITION=premium NEXT_PUBLIC_CODEX_DEFAULT_MODE=local next build && node scripts/fix-standalone-symlinks.js && pnpm electron:build",
    "electron:pack": "pnpm electron:prebuild && electron-builder --dir",
    "electron:dist": "pnpm electron:prebuild && electron-builder",
    "electron:dist:mac": "pnpm electron:prebuild && electron-builder --mac",
    "electron:dist:mac:unsigned": "pnpm electron:prebuild && electron-builder --mac -c.mac.identity=null",
    "electron:dist:win": "pnpm electron:prebuild && electron-builder --win",
    "electron:dist:linux": "pnpm electron:prebuild && electron-builder --linux",
    "cap:prebuild": "node scripts/capacitor-build.js",
    "cap:sync": "npx cap sync",
    "cap:build": "pnpm cap:prebuild && pnpm cap:sync",
    "cap:ios": "pnpm cap:build && npx cap open ios",
    "cap:android": "pnpm cap:build && npx cap open android",
    "cap:ios:build": "pnpm cap:build && cd ios/App && xcodebuild -workspace App.xcworkspace -scheme App -configuration Release -archivePath build/Quarry.xcarchive archive",
    "cap:android:build": "pnpm cap:build && cd android && ./gradlew assembleRelease"
  },
