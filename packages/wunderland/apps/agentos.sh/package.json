{
  "name": "@framersai/agentos.sh",
  "version": "0.1.0",
  "private": true,
  "description": "Marketing site for AgentOS (agentos.sh) built with Next.js, React, and Tailwind CSS.",
  "scripts": {
    "dev": "next dev",
    "dev:full": "concurrently \"pnpm --filter @framers/agentos run docs:watch\" \"next dev\"",
    "build": "npm run copy:docs || true && next build && node scripts/post-export.mjs",
    "copy:docs": "node scripts/copy-docs.js",
    "export-chrome": "node scripts/export-static-chrome.mjs",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "docs:watch": "pnpm --filter @framers/agentos run docs:watch",
    "export": "next build"
  },
  "dependencies": {
    "@types/react-syntax-highlighter": "^15.5.13",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.23",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.469.0",
    "mermaid": "^10.9.4",
    "critters": "^0.0.22",
    "next": "^14.2.15",
    "next-intl": "^4.5.3",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.12",
    "@types/node": "^20.15.0",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.15",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "packageManager": "pnpm@10.15.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/framersai/agentos.sh"
  }
}
