{
  "compilerOptions": {
    "composite": true,
    "rootDir": "src",
    "baseUrl": "./src",
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2020",
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@framers/agentos/*": ["./*"],
      "@prisma/client": ["./stubs/prismaClient.ts"],
      "dompurify": ["./stubs/dompurify.d.ts"],
      "@framers/sql-storage-adapter": ["../../sql-storage-adapter/dist/index.d.ts"],
      "@framers/sql-storage-adapter/*": ["../../sql-storage-adapter/dist/*"],
      "graphology": ["./stubs/graphology.d.ts"],
      "graphology-communities-louvain": ["./stubs/graphology-communities-louvain.d.ts"],
      "hnswlib-node": ["./stubs/hnswlib-node.d.ts"]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
