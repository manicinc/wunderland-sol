{
  "$schema": "https://agentos.sh/schemas/extension-manifest-v1.json",
  "id": "com.framers.ext.template",
  "name": "Template Extension",
  "version": "1.0.0",
  "description": "A template for creating new AgentOS extensions",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com",
    "url": "https://github.com/yourusername"
  },
  "license": "MIT",
  "keywords": ["template", "example"],
  "agentosVersion": "^2.0.0",
  "categories": ["utility"],
  "repository": {
    "type": "git",
    "url": "https://github.com/framersai/agentos-extensions"
  },
  "extensions": [
    {
      "kind": "tool",
      "id": "exampleTool",
      "displayName": "Example Tool",
      "description": "An example tool implementation",
      "entry": "./dist/tools/exampleTool.js"
    }
  ],
  "configuration": {
    "properties": {
      "template.apiKey": {
        "type": "string",
        "description": "API key for external service",
        "secret": true
      },
      "template.endpoint": {
        "type": "string",
        "default": "https://api.example.com",
        "description": "API endpoint URL"
      }
    }
  }
}
