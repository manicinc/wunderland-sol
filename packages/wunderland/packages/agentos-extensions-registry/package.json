{
  "name": "@framers/agentos-extensions-registry",
  "version": "0.1.1",
  "files": [
    "dist",
    "README.md"
  ],
  "description": "Curated extension registry bundle for AgentOS â€” single import to register channels, tools, and integrations",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./channels": {
      "import": "./dist/channel-registry.js",
      "types": "./dist/channel-registry.d.ts"
    },
    "./providers": {
      "import": "./dist/provider-registry.js",
      "types": "./dist/provider-registry.d.ts"
    },
    "./tools": {
      "import": "./dist/tool-registry.js",
      "types": "./dist/tool-registry.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/framersai/agentos-extensions-registry.git"
  },
  "keywords": [
    "agentos",
    "extensions",
    "registry",
    "channels",
    "tools"
  ],
  "author": {
    "name": "Framers AI",
    "email": "team@frame.dev",
    "url": "https://frame.dev"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@framers/agentos": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@framers/agentos": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@framers/agentos-ext-auth": "^1.0.0",
    "@framers/agentos-ext-skills": "^1.0.0",
    "@framers/agentos-ext-web-search": "^1.0.0",
    "@framers/agentos-ext-web-browser": "^1.0.0",
    "@framers/agentos-ext-telegram": "^1.0.0",
    "@framers/agentos-ext-cli-executor": "^1.0.0",
    "@framers/agentos-ext-giphy": "^1.0.0",
    "@framers/agentos-ext-image-search": "^1.0.0",
    "@framers/agentos-ext-voice-synthesis": "^1.0.0",
    "@framers/agentos-ext-news-search": "^1.0.0"
  },
  "devDependencies": {
    "@framers/agentos": "^0.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.0.0"
  }
}
